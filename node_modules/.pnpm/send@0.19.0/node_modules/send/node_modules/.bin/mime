#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/vishal/70e81f63-2ef4-4d42-8a59-14983771aa9b/cjtech/blockchain/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/media/vishal/70e81f63-2ef4-4d42-8a59-14983771aa9b/cjtech/blockchain/node_modules/.pnpm/mime@1.6.0/node_modules:/media/vishal/70e81f63-2ef4-4d42-8a59-14983771aa9b/cjtech/blockchain/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/vishal/70e81f63-2ef4-4d42-8a59-14983771aa9b/cjtech/blockchain/node_modules/.pnpm/mime@1.6.0/node_modules/mime/node_modules:/media/vishal/70e81f63-2ef4-4d42-8a59-14983771aa9b/cjtech/blockchain/node_modules/.pnpm/mime@1.6.0/node_modules:/media/vishal/70e81f63-2ef4-4d42-8a59-14983771aa9b/cjtech/blockchain/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
else
  exec node  "$basedir/../../../../../mime@1.6.0/node_modules/mime/cli.js" "$@"
fi
